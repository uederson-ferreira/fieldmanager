{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "fieldmanager",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules .pnpm-store dist",
    "fresh": "pnpm clean && pnpm install",
    "migrate:perguntas": "node scripts/migracao_perguntas.js",
    "migrate:sql": "node scripts/executar_sql.js",
    "test:version": "node scripts/teste_version_api.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@supabase/supabase-js": "^2.50.2",
    "@tanstack/react-query": "^5.81.2",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^22.15.33",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "dexie": "^4.0.11",
    "dotenv": "^17.0.1",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "idb": "^8.0.3",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5",
    "recharts": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "uuid": "^11.1.0",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.0.0",
    "workbox-webpack-plugin": "^7.3.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.29.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "jsdom": "^27.2.0",
    "markdownlint-cli": "^0.45.0",
    "supabase": "^2.31.4",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "^7.0.0",
      "lru-cache": "^7.18.3"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "7"
      }
    }
